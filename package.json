{
  "name": "flipper-webview",
  "displayName": "Flipper Webview",
  "description": "A VSCode extension to host the Flipper webview",
  "version": "0.0.1",
  "publisher": "dumpysquare",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flipperWebview.start",
        "title": "Start Flipper Webview"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "lint": "tsc --noEmit && eslint 'src/**/*.{js,ts,tsx}' 'tests/**/*.{js,ts,tsx}'",
    "watch": "tsc -watch -p ./",
    "compile": "tsc -p ./",
    "test": "nyc mocha -r ts-node/register \"tests/*.tests.ts\"",
    "build-code-docs": "typedoc --out code_docs src",
    "build-package": "npm run compile && npm pack",
    "publish-package": "npm run compile && npm publish"
  },
  "devDependencies": {
    "@types/assert": "^1.5.10",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.4",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "assert": "^2.1.0",
    "eslint": "^8.57.0",
    "mocha": "^10.4.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@f5devcentral/f5-fast-core": "^0.23.0",
    "deepmerge-ts": "^5.1.0"
  }
}
